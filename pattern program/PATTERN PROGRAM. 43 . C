#include<stdio.h>
#include<conio.h>
void main()
{
   int j,k,n=5;
   char i;
   clrscr();
   for(i=n;i>=1;i--)
   {
     for(j=n-1;j>=i;j--)
     {
       printf(" ");
     }
     for(k=1;k<=i;k++)
     {
       printf("%c ",i+64);
     }
     printf("\n");
   }
   getch();
}

PROGRAM WITH ASCCI CODE :-

OUTPUT :- E E E E E
           D D D D
            C C C
             B B
              A
